;Eagles Revenge.per

(defrule
(goal g-current-strategy  EAGLES-REVENGE)
(goal g-age-status TO-CASTLE)
(can-afford-research ri-eagle-warrior)
=>
(set-goal g-desired-num-eagle-warrior 5)
(set-goal g-desired-num-military 5)
(set-goal g-primary-unit eagle-warrior)
)

(defrule
(goal g-current-strategy  EAGLES-REVENGE)
(up-compare-goal g-age-status >= EARLY-CASTLE)
=>
(set-goal g-desired-num-eagle-warrior 20)
(set-goal g-desired-num-military 20)
(set-goal g-primary-unit eagle-warrior)
)